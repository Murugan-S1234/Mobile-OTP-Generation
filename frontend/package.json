{
  "name": "frontend",
  "version": "0.1.0",
  "homepage": "https://Murugan-S1234.github.io/Mobile-OTP-Generation",
  "private": true,
  "dependencies": {
    "axios": "^1.7.9",
    "cra-template": "1.2.0",
    "cross-env": "^7.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^4.0.3",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider CHOKIDAR_USEPOLLING=true react-scripts start --host 0.0.0.0",
    "dev": "concurrently \"npm start\" \"npm run client\"",
    "client": "set NODE_OPTIONS=--openssl-legacy-provider && npm start --prefix ../frontend",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --depth 1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "gh-pages": "^6.2.0"
  }
}
